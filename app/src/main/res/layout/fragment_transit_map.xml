<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <fragment
        android:id="@+id/map_fragment"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="jessevivanco.com.pegcitytransit.ui.activities.MapsActivity"/>

    <View
        android:layout_width="@dimen/material_spacing_small"
        android:layout_height="@dimen/material_spacing_small"
        android:layout_gravity="center"
        android:background="@drawable/map_center"/>

</FrameLayout>